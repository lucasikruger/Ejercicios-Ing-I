!Track methodsFor: 'car position' stamp: 'SCB 5/2/2022 20:36:00' prior: 50477002!
put: aCar at: aPosition

	| acc |
	
	self assertTrackLengthBiggerThanPosition: aPosition.

	self remove: aCar.
	acc _ 0 * kilometer.

	sectors do: [ :sector |
				(aPosition - acc <= (sector length)) ifTrue: [^sector put: aCar at: aPosition - acc].
				acc _ acc + sector length].! !

